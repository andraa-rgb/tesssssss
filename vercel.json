    "framework": null,
    "functions": {
        "api/index.php": { 
            "runtime": "vercel-php@0.7.4",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(css|js|vendor|assets|build)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
